<div>

	<div class="admin_title">Signed up (<%= @user_count %>)</div>
	<div class="preorder">
		<% @users.each do |user| %>
			<div><%= user.email %></div>
		<% end %>
	</div>

	
	<div class="admin_title">Messages (<%= @message_count %>)</h4>
	<div class="contact_messages">
		<% @contacts.each do |contact| %>
			<div class="contact_email"><%= contact.email %></div>
			<% contact.messages.each do |message| %>
				<div><%= message.content %></div>
			<% end %>
			<br>
		<% end %>
	</div>

</div>